<Sound_theme>
	<Version>1.0</Version>
	<Name>MusicalBox</Name>
	<Description>A smoothly increasing loving sound coming from a music box.</Description>
	<Author>Lauber</Author>
	<REM>
		<Action>
			<Type>Initialization</Type>
			<AdjustVolume>1</AdjustVolume>
		</Action>
		<Action>
			<Type>Blinking_light</Type>
		</Action>
		<Action>
			<Type>Sound</Type>
			<SoundResource>ChildrensBox</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
		</Action>
		<Action>
			<Type>Pause</Type>
			<Length>1-10</Length>
		</Action>
		<Action>
			<Type>Volume</Type>
			<AdjustVolumeByPhaseLength>104</AdjustVolumeByPhaseLength>
		</Action>
	</REM>
	<Wake_up>
		<Action>
			<Type>Initialization</Type>
			<AdjustVolume>104</AdjustVolume>
		</Action>
		<Action>
			<Type>Blinking_light</Type>
			<BgColor>RGB:255,255,255</BgColor>
		</Action>
		<Phase>
			<Rounds>200</Rounds>
			<Length>60</Length>
			<Phase>
				<Rounds>3</Rounds>
				<Action>
					<Type>Sound</Type>
					<SoundResource>BeepBeep</SoundResource>
					<SoundResourceType>mp3</SoundResourceType>
				</Action>
			</Phase>
			<Action>
				<Type>Pause</Type>
				<Length>5</Length>
			</Action>
			<Action>
				<Type>Volume</Type>
				<AdjustVolume>+11</AdjustVolume>
			</Action>
		</Phase>
		<Phase>
			<Rounds>200</Rounds>
			<Length>60</Length>
			<Phase>
				<Rounds>3</Rounds>
				<Action>
					<Type>Sound</Type>
					<SoundResource>BeepBeepFast</SoundResource>
					<SoundResourceType>mp3</SoundResourceType>
				</Action>
			</Phase>
			<Action>
				<Type>Volume</Type>
				<AdjustVolume>+11</AdjustVolume>
			</Action>
		</Phase>
	</Wake_up>
	<Snooze_interrupt>
		<Action>
			<Type>Initialization</Type>
			<AdjustVolume>42</AdjustVolume>
		</Action>
		<Action>
			<Type>Sound</Type>
			<SoundResource>ChildrensBox</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
			<RelativeVolume>-8-+8</RelativeVolume>
		</Action>
		<Action>
			<Type>Pause</Type>
			<Length>30-45</Length>
		</Action>
	</Snooze_interrupt>
	<Shock_alarm>
		<Action>
			<Type>Sound</Type>
			<SoundResource>Trumpet</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
			<HardVolume>100</HardVolume>
		</Action>
	</Shock_alarm>
</Sound_theme>
