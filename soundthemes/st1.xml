<Sound_theme>
	<Name>Nature</Name>
	<Version>1.0</Version>
	<Description>Nature sounds (test)</Description>
	<Author>Lauber</Author>
	<REM>
		<Phase>
			<Phase>
				<bPhase>
					<Order>Random</Order>
					<Rounds>2</Rounds>
					<Action>
						<Type>Blinking_light</Type>
						<Frequency>20</Frequency>
						<FgColor>RGB:12,80,180</FgColor>
						<BgColor>RGB:12,80,80</BgColor>
					</Action>
					<Action>
						<Type>Pause</Type>
						<Length>3</Length>
					</Action>
				</bPhase>
				<Phase>
					<!-- Order = Random tells that _one_ child will be picked at random, not that all children will be gone through in random order.  -->
					<Rounds>2</Rounds>
					<Action>
						<Type>Volume</Type>
						<AdjustVolume>20</AdjustVolume>
					</Action>
					<Action>
						<Probability>50</Probability>
						<Type>Sound</Type>
						<SoundResource>rem2</SoundResource>
						<SoundResourceType>mp3</SoundResourceType>
						<aVolume>50</aVolume>
						<aLength>50</aLength>
					</Action>
					<aAction>
						<Type>Pause</Type>
						<Length>3</Length>
					</aAction>
					<Action>
						<Type>Steady_light</Type>
						<FgColor>RGB:45,30,1</FgColor>
						<BgColor>RGB:12,80,80</BgColor>
					</Action>
					<Action>
						<Type>Pause</Type>
						<Length>5</Length>
					</Action>
					<Action>
						<Type>Blinking_light</Type>
						<Frequency>20</Frequency>
						<FgColor>RGB:121,180,180</FgColor>
						<BgColor>RGB:12,80,180</BgColor>
					</Action>
					<Action>
						<Type>Volume</Type>
						<AdjustVolume>20</AdjustVolume>
					</Action>
					<Action>
						<Probability>50</Probability>
						<Type>Sound</Type>
						<SoundResource>rem2</SoundResource>
						<SoundResourceType>mp3</SoundResourceType>
						<Volume>20</Volume>
						<Length>10</Length>
					</Action>
					<aAction>
						<Type>Pause</Type>
						<Length>2</Length>
					</aAction>
				</Phase>
			</Phase>
		</Phase>
	</REM>
	<Wake_up>
		<Phase>
			<Action>
				<Type>Sound</Type>
				<SoundResource>alarm</SoundResource>
				<SoundResourceType>mp3</SoundResourceType>
				<Volume>30</Volume>
				<Length>10</Length>
			</Action>
			<Action>
				<Type>Blinking_light</Type>
				<Frequency>20</Frequency>
				<FgColor>RGB:120,60,180</FgColor>
				<BgColor>RGB:12,200,60</BgColor>
			</Action>
			<Action>
				<Type>Sound</Type>
				<SoundResource>alarm2</SoundResource>
				<SoundResourceType>mp3</SoundResourceType>
				<Volume>30</Volume>
				<Length>10</Length>
			</Action>
			<Action>
				<Type>Blinking_light</Type>
				<Frequency>20</Frequency>
				<FgColor>RGB:180,180,80</FgColor>
				<BgColor>RGB:180,180,180</BgColor>
			</Action>
			<Action>
				<Type>Sound</Type>
				<SoundResource>alarm3</SoundResource>
				<SoundResourceType>mp3</SoundResourceType>
				<Volume>30</Volume>
				<Length>10</Length>
			</Action>
			<Action>
				<Type>Blinking_light</Type>
				<Frequency>20</Frequency>
				<FgColor>RGB:12,80,250</FgColor>
				<BgColor>RGB:170,170,80</BgColor>
			</Action>
		</Phase>
	</Wake_up>
	<Snooze_interrupt>
		<Phase>
			<Action>
				<Type>Sound</Type>
				<SoundResource>snooze</SoundResource>
				<SoundResourceType>mp3</SoundResourceType>
				<Volume>150</Volume>
				<Length>10</Length>
			</Action>
			<Action>
				<Type>Blinking_light</Type>
				<Frequency>20</Frequency>
				<FgColor>RGB:120,60,180</FgColor>
				<BgColor>RGB:12,200,60</BgColor>
			</Action>
			<Action>
				<Type>Pause</Type>
				<Length>20</Length>
			</Action>
			<aAction>
				<Type>Sound</Type>
				<SoundResource>snooze2</SoundResource>
				<SoundResourceType>mp3</SoundResourceType>
				<Volume>30</Volume>
				<Length>10</Length>
			</aAction>
			<Action>
				<Type>Blinking_light</Type>
				<Frequency>20</Frequency>
				<FgColor>RGB:180,180,80</FgColor>
				<BgColor>RGB:180,180,180</BgColor>
			</Action>
			<Action>
				<Type>Pause</Type>
				<Length>20</Length>
			</Action>
		</Phase>
	</Snooze_interrupt>
	<Shock_alarm>
		<Phase>
			<Action>
				<Type>Blinking_light</Type>
				<Frequency>20</Frequency>
				<FgColor>RGB:225,30,180</FgColor>
				<BgColor>RGB:80,40,250</BgColor>
			</Action>
			<Action>
				<Type>Sound</Type>
				<SoundResource>shock</SoundResource>
				<SoundResourceType>mp3</SoundResourceType>
				<Volume>100</Volume>
				<Length>10</Length>
			</Action>
			<Action>
				<Type>Pause</Type>
				<Length>2</Length>
			</Action>
		</Phase>
	</Shock_alarm>
</Sound_theme>
