<Sound_theme>
	<Version>1.0</Version>
	<Name>Birds</Name>
	<Description>Birds singing in the trees. Smoothly increasing sound level.
	</Description>
	<Author>Lauber</Author>
	<REM>
		<Action>
			<Type>Initialization</Type>
			<AdjustVolume>1</AdjustVolume>
		</Action>
		<Action>
			<Type>Blinking_light</Type>
		</Action>
		<Action>
			<Type>Sound</Type>
			<SoundResource>Birds</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
		</Action>
		<Action>
			<Type>Pause</Type>
			<Length>1-10</Length>
		</Action>
		<Action>
			<Type>Volume</Type>
			<AdjustVolumeByPhaseLength>149</AdjustVolumeByPhaseLength>
		</Action>
	</REM>
	<Wake_up>
		<Action>
			<Type>Initialization</Type>
			<AdjustVolume>80</AdjustVolume>
		</Action>
		<Action>
			<Type>Blinking_light</Type>
			<BgColor>RGB:255,255,255</BgColor>
		</Action>
		<Action>
			<Type>Sound</Type>
			<SoundResource>Ring</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
		</Action>
		<Action>
			<Type>Pause</Type>
			<Length>5</Length>
		</Action>
		<Action>
			<Type>Volume</Type>
			<AdjustVolume>+16</AdjustVolume>
		</Action>
	</Wake_up>
	<Snooze_interrupt>
		<Action>
			<Type>Initialization</Type>
			<AdjustVolume>56</AdjustVolume>
		</Action>
		<Action>
			<Type>Sound</Type>
			<SoundResource>Birds</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
			<RelativeVolume>-8-+8</RelativeVolume>
		</Action>
		<Action>
			<Type>Pause</Type>
			<Length>30-45</Length>
		</Action>
	</Snooze_interrupt>
	<Shock_alarm>
		<Action>
			<Type>Sound</Type>
			<SoundResource>Trumpet</SoundResource>
			<SoundResourceType>mp3</SoundResourceType>
			<HardVolume>100</HardVolume>
		</Action>
	</Shock_alarm>
</Sound_theme>
